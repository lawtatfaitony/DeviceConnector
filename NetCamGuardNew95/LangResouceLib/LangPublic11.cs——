				}


				 ///<summary>密碼太短</summary>
				public static string User_Password_Format_TooShort
				{
					get
					{

						 switch (LangUtilities.LanguageCode)
							{
								 case "zh-CN":
									 return "密码太短";
								 case "zh-HK":
									 return "密碼太短";
								  case "en-US":
									 return "Password required the min 6 letter";
 								 
 								 default:									 
									 return "Password required the min 6 letter";
							} 
 					}
				}


				 ///<summary>登錄用戶名</summary>
				public static string User_UserName
				{
					get
					{

						 switch (LangUtilities.LanguageCode)
							{
								 case "zh-CN":
									 return "登录用户名";
								 case "zh-HK":
									 return "登錄用戶名";
								  case "en-US":
									 return "UserName";
 								 
 								 default:									 
									 return "UserName";
							} 
 					}
				}


				 ///<summary>用戶名或者密碼格式錯誤(不允許空)</summary>
				public static string User_UserNamePasswordNotNull
				{
					get
					{

						 switch (LangUtilities.LanguageCode)
							{
								 case "zh-CN":
									 return "用户名或者密码格式错误(不允许空)";
								 case "zh-HK":
									 return "用戶名或者密碼格式錯誤(不允許空)";
								  case "en-US":
									 return "Username or Password is not allowed null";
 								 
 								 default:									 
									 return "Username or Password is not allowed null";
							} 
 					}
				}


				 ///<summary>登錄用戶必須是數字和字母</summary>
				public static string User_UserName_Format
				{
					get
					{

						 switch (LangUtilities.LanguageCode)
							{
								 case "zh-CN":
									 return "登录用户必须是数字和字母";
								 case "zh-HK":
									 return "登錄用戶必須是數字和字母";
								  case "en-US":
									 return "Letter Or Digits";
 								 
 								 default:									 
									 return "Letter Or Digits";
							} 
 					}
				}


        #endregion
         
    }

}
