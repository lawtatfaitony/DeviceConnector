				 ///<summary>忘記密碼</summary>
				public static string Account_ForgetPassword
				{
					get
					{

						 switch (LangUtilities.LanguageCode)
							{
								 case "zh-CN":
									 return "忘记密码";
								 case "zh-HK":
									 return "忘記密碼";
								  case "en-US":
									 return "Forget Password";
 								 
 								 default:									 
									 return "Forget Password";
							} 
 					}
				}


